      "pathType": "local",
      "name": "data",
      "format": "csv",
      "mediatype": "text/csv",
      "encoding": "ISO-8859-1",
      "dialect": {
        "delimiter": ",",
        "quoteChar": "\""
      },
      "schema": {
        "fields": [
          {
            "name": "Code value",
            "type": "integer",
            "format": "default"
          },
          {
            "name": "Character name",
            "type": "string",
            "format": "default"
          },
          {
            "name": "General category",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Canonical combining classes",
            "type": "integer",
            "format": "default"
          },
          {
            "name": "Bidirectional category",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Character decomposition mapping",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Decimal digit value",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Digit value",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Numeric value",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Mirrored",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Unicode 1.0 Name",
            "type": "string",
            "format": "default"
          },
          {
            "name": "10646 comment field",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Uppercase mapping",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Lowercase mapping",
            "type": "string",
            "format": "default"
          },
          {
            "name": "Titlecase mapping",
            "type": "string",
            "format": "default"
          }
        ],
        "missingValues": [
          ""
        ]
      }
    }
  ],
  "description": "UnicodeData.txt is a reference file from the Unicode Consortium containing metadata for every Unicode character, including code points, names, categories, and properties, used to support Unicode-compliant text processing.",
  "collection": "reference-data",
